BUILD_SYSTEM=''
HOST_SYSTEM=''
PREPROC_C='gcc -E '
MKDEP_C='gcc -MM '
COMPILE_C='gcc -c '
PREPROC_OBJC=''
MKDEP_OBJC=''
COMPILE_OBJC=''
LINK='gcc '
MAKE='make'
ECHON='echo -n'
SED='sed'
REZ=''
WINDRES=''
uqm_CFLAGS=' -I/usr/include/SDL -D_GNU_SOURCE=1 -D_REENTRANT -I/usr/include/SDL -D_GNU_SOURCE=1 -D_REENTRANT -O3 -DNDEBUG -DGFXMODULE_SDL -DHAVE_OPENGL  -pthread -D_REENTRANT   -DNETPLAY=NETPLAY_FULL  -DHAVE_JOYSTICK -DHAVE_ZIP=1   -DUSE_PLATFORM_ACCEL -DTHREADLIB_SDL -I"."'
uqm_LDFLAGS=' -L/usr/lib/i386-linux-gnu -lSDL -L/usr/lib/i386-linux-gnu -lSDL -lSDL_image -lGL -L/usr/lib -lmikmod -ldl -lvorbisfile -lvorbis -lm -logg    -lz  '
uqm_INSTALL_BINDIR=$DESTDIR'/usr/games/'
uqm_INSTALL_LIBDIR=$DESTDIR'/usr/lib/games/'
uqm_INSTALL_SHAREDIR=$DESTDIR'/usr/share/games/'

# Exported variables are available from all the Makeinfo files
# Non-exported files only where build.vars is explicitely included.
uqm_SOUNDMODULE='mixsdl'
uqm_OGGVORBIS='vorbisfile'
uqm_USE_INTERNAL_MIKMOD=''
uqm_HAVE_GETOPT_LONG='#define HAVE_GETOPT_LONG'
uqm_HAVE_REGEX='1'
uqm_GFXMODULE='sdl'
uqm_HAVE_OPENGL='1'
uqm_USE_ZIP_IO='1'
uqm_USE_PLATFORM_ACCEL='1'
uqm_THREADLIB='SDL'
uqm_NETPLAY='FULL'
uqm_USE_WINSOCK=''
DEBUG='0'
MACRO_WIN32=''
MACRO___MINGW32__=''
export BUILD_SYSTEM HOST_SYSTEM DEBUG
export MACRO_WIN32 MACRO___MINGW32__
export uqm_SOUNDMODULE uqm_USE_INTERNAL_MIKMOD uqm_HAVE_GETOPT_LONG
export uqm_HAVE_REGEX uqm_USE_WINSOCK uqm_GFXMODULE uqm_HAVE_OPENGL
export uqm_USE_ZIP_IO uqm_USE_PLATFORM_ACCEL uqm_THREADLIB uqm_NETPLAY

